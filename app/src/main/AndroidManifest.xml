<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.tabsample"
    android:versionCode="1"
    android:versionName="1.0">

    <uses-sdk android:minSdkVersion="14" />

    <application
        android:icon="@drawable/icon"
        android:theme="@style/app_theme">
        <activity
            android:name="app.activities.MyTabActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="app.activities.DetectionActivity" />
        <activity android:name="app.activities.DataActivity" />
        <activity
            android:name="app.activities.CommunityActivity"
            android:label="@string/title_activity_community" />
        <activity
            android:name="app.activities.MeActivity"
            android:label="@string/title_activity_me" />

        <service
            android:name="app.activities.MyService"
            android:enabled="true"
            android:exported="true"></service>
    </application>

</manifest>